{
  "name": "ziro-cli",
  "version": "0.0.7",
  "description": "A cli tool for the ziro projects",
  "main": "cli/src/index.js",
  "scripts": {
    "build": "rollup cli/ziro.js --file dist/ziro.js --format esm",
    "watch": "rollup cli/ziro.js --file dist/ziro.js --format esm --watch"
  },
  "bin": {
    "ziro": "dist/ziro.js"
  },
  "keywords": [
    "cli",
    "web-components",
    "web-app"
  ],
  "type": "module",
  "author": "Alex Lockhart",
  "license": "ISC",
  "devDependencies": {
    "chalk": "^4.1.0",
    "prompts": "^2.4.1",
    "rollup": "^3.2.3"
  }
}
